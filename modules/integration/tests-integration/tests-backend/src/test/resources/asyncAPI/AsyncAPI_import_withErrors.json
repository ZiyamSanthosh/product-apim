{
  "syncapi": "2.0.0",
  "info": {
    "title": "Echo Server 2",
    "version": "1.0"
  },
  "servers": {
    "localhost": {
      "url": "wss://echo.websocket.org:443",
      "protocol": "ws"
    }
  },
  "channels": {
    "/echo2": {
      "subscribe": {
        "operationId": "onEcho",
        "message": {
          "$ref": "#/components/messages/echo"
        }
      },
      "publish": {
        "operationId": "sendEcho",
        "message": {
          "$ref": "#/components/messages/echo"
        }
      }
    }
  },
  "components": {
    "messages": {
      "echo": {
        "payload": {
          "type": "string"
        }
      }
    }
  }
}